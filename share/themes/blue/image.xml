<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<theme geometry="800x600">

    <application name="imageviewer" style="simple">
        <properties depth="1"/>
        <image x="0" y="0" width="800" height="600" filename="$image">
	    <properties xalign="center" yalign="center" keep-aspect="true"/>
	</image>
    </application>


</theme>
